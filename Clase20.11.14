package com.superficies201014;
/**
CLASE DONDE CALCULAMOS LAS AREAS DE UN CUADRADO
RESGUARDO TXT ARCHIVO SE ENCUENTRA EN LOS PROYECTOS
*/
public class Area {
private Cuadrado c;
public Area(Cuadrado c) {
this.c = c;
}
public Cuadrado getC() {
return c;
}
public void setC(Cuadrado c) {
this.c = c;
}
public int calcular(){
int area=c.getLado()*c.getLado();
return area;
}
}
